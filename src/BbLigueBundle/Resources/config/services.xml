<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <parameters>
        <parameter key="bb.rosters.class">BbLigueBundle\Services\RostersService</parameter>
    </parameters>

    <services>
		<service id="bb.rosters" class="%bb.rosters.class%">
			<argument>%bb_ligue.rules%</argument>
		</service>
        <service id="obblm.form.type.team" class="BbLigueBundle\Form\Type\Team">
            <tag name="form.type" alias="team" />
        </service>
    </services>
</container>
