                </th>
            </tr>
            {% for pj in journey.availaiblePlayers %}
            <tr>
                <td>{{ pj.player.position }}</td>
                <td>{{ pj.player.name }}</td>
                <td>{% set player_type = 'blood_bowl.rosters.' ~ team.roster ~ '.positions.' ~ pj.player.type %}{{ player_type|trans({}, 'bb-dictionnary' ) }}</td>
                <td>{{ pj.move }}</td>
                <td>{{ pj.strenght }}</td>
                <td>{{ pj.agility }}</td>
                <td>{{ pj.average }}</td>
                <td>{{ pj.skills|skills|raw }}</td>
                <td>{{ pj.injuries|injuries|raw }}</td>
                <td>{{ pj.completions }}</td>
                <td>{{ pj.touchdowns }}</td>
                <td>{{ pj.interceptions }}</td>
                <td>{{ pj.casualties }}</td>
                <td>{{ pj.mvps }}</td>
                <td>{{ pj.spp }}</td>
                <td>{{ pj.value|bbprice }}</td>
            </tr>
            {% endfor %}
        </table>
        <table class="informations" style="display: table;">
            <tr>
                <th>TR</th>
                <td>{{ journey.tr }}</td>
            </tr>
            <tr>
                <th>Treasure</th>
                <td>{{ journey.treasure|bbkpo|raw }}</td>
            </tr>
            <tr>
                <th>Rerolls</th>
                <td>{{ journey.rerolls }} * {{ journey.team.getBaseRerollValue }} = {{ journey.getRerollsValue }}</td>
            </tr>
            <tr>
                <th>Pop</th>
                <td>{{ journey.popularity }}</td>
            </tr>
            <tr>
                <th>Cheerleaders</th>
                <td>{{ journey.cheerleaders }}</td>
            </tr>
            <tr>
                <th>Assistants</th>
                <td>{{ journey.assistants }}</td>
            </tr>
        </table>
