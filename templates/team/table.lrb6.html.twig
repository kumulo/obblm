<table>
    <tr>
        <th>#</th>
        <th>Name</th>
        <th>Type</th>
        <th>{{ 'lrb6.trait.ma.abbr'|trans({}, 'lrb6') }}</th>
        <th>{{ 'lrb6.trait.st.abbr'|trans({}, 'lrb6') }}</th>
        <th>{{ 'lrb6.trait.ag.abbr'|trans({}, 'lrb6') }}</th>
        <th>{{ 'lrb6.trait.av.abbr'|trans({}, 'lrb6') }}</th>
        <th>{{ 'lrb6.trait.skill.title'|trans({}, 'lrb6') }}</th>
        <th>{{ 'lrb6.trait.injury.title'|trans({}, 'lrb6') }}</th>
        <th>{{ 'lrb6.trait.value.title'|trans({}, 'lrb6') }}</th>
    </tr>
    {% for player in team.players %}
        {% include 'team/player-line.lrb6.html.twig' with { 'player': player } %}
    {% else %}
        <tr colspan="5">No players</tr>
    {% endfor %}
</table>